<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:id="@+id/rlShinBoard"
    android:background="#F6F6F8">
    <FrameLayout 
        android:id="@+id/frameLayout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        />
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" 
        android:layout_alignParentBottom="true">
        <ImageView 
            android:id="@+id/inviteFragmentButtonImage"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@drawable/noticeboard_invitebutton"
            />
        <TextView 
            android:id="@+id/inviteFragmentButtonText"
            android:layout_width="60dp"
            android:layout_height="24dp"
            android:layout_below="@+id/inviteFragmentButtonImage"
            android:textSize="15sp"
            android:textColor="#778899"
            android:gravity="center"
            android:text="@string/noticeboardinvitebutton"
            />
        <ImageView 
            android:id="@+id/inviteFragmentButtonArea"
            android:layout_width="48dp"
            android:layout_height="48dp"
            />
        <ImageView 
            android:id="@+id/rewardFragmentButtonImage"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@drawable/noticeboard_rewardbutton"
            />
        <TextView 
            android:id="@+id/rewardFragmentButtonText"
            android:layout_width="60dp"
            android:layout_height="24dp"
            android:layout_below="@+id/rewardFragmentButtonImage"
            android:textSize="15sp"
            android:textColor="#778899"
            android:gravity="center"
            android:text="@string/noticeboardrewardbutton"
            />
        <ImageView 
            android:id="@+id/rewardFragmentButtonArea"
            android:layout_width="48dp"
            android:layout_height="48dp"
            />
        <ImageView 
            android:id="@+id/shareFragmentButtonImage"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@drawable/noticeboard_sharebutton"
            />
        <TextView 
            android:id="@+id/shareFragmentButtonText"
            android:layout_width="60dp"
            android:layout_height="24dp"
            android:layout_below="@+id/shareFragmentButtonImage"
            android:textSize="15sp"
            android:textColor="#778899"
            android:gravity="center"
            android:text="@string/noticeboardsharebutton"
            />
        <ImageView 
            android:id="@+id/shareFragmentButtonArea"
            android:layout_width="48dp"
            android:layout_height="48dp"
            />
        <ImageView 
            android:id="@+id/noticeFragmentButtonImage"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:background="@drawable/noticeboard_noticebutton"
            />
        <TextView 
            android:id="@+id/noticeFragmentButtonText"
            android:layout_width="60dp"
            android:layout_height="24dp"
            android:layout_below="@+id/shareFragmentButtonImage"
            android:textSize="15sp"
            android:textColor="#778899"
            android:gravity="center"
            android:text="@string/noticeboardnoticebutton"
            />
        <ImageView 
            android:id="@+id/noticeFragmentButtonArea"
            android:layout_width="48dp"
            android:layout_height="48dp"
            />
    </RelativeLayout>
    <ImageView 
	    android:id="@+id/loadingImage"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent"
	    android:visibility="invisible"
	    android:src="@drawable/notice_loading"
	    />

</RelativeLayout>
